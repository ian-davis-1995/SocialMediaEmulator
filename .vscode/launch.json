{
  "configurations": [
    {
      "command": "npm run start-experiment",
      "name": "Experiment Debug",
      "request": "launch",
      "type": "node-terminal",
      "cwd": "${workspaceFolder}/digital_deception_emulator/frontend/emulator"
    },
    {
      "name": "Python: Current File",
      "type": "python",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "cwd": "${workspaceRoot}",
      "env": {
        "PYTHONPATH": "${workspaceRoot}"
      }
    }
  ]
}
